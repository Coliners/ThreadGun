<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net472;net46;net45;netstandard2.0</TargetFrameworks>
    <Version>1.4.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Copyright © RexProg 2019</Copyright>
    <Company>RexProg</Company>
    <Authors>RexProg</Authors>
    <Description>ThreadGun is a form of multithreading created for developers keen on building fast and stable application.</Description>
    <PackageProjectUrl>https://github.com/RexProg/ThreadGun</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RexProg/ThreadGun</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ThreadGun MultiThreading MultiThread Thread Threading</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageIconUrl>https://software.intel.com/sites/default/files/managed/ce/40/multithreaded-icon-375w.png</PackageIconUrl>
  </PropertyGroup>
</Project>
